@model IEnumerable<ECV.Models.Deneyim>
<div class="col-lg-12">
    <div class="card">
        <div class="card-header">
            <h3 class="card-title">DENEYİM Listeleniyor</h3>
        </div>
        <!-- /.card-header -->
        <div class="card-body">
            <table id="example1" class="table table-bordered table-striped">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Firma Adı</th>
                        <th>Unvan</th>
                        <th>Yıl Aralığı</th>
                        <th>Tecrübe</th>
                        <th>İşlem</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {

                        <tr>
                            <td>@item.DID</td>
                            <td>@item.firmaAdi</td>
                            <td>@item.unvan</td>
                            <td>@item.yil</td>
                            <td>@item.tecrübe</td>
                            <td>
                                <div class="btn-group">
                                    <a href="/CV/DeneyimSil?DID=@item.DID" class="btn btn-danger">Sil</a>
                                    <a href="/CV/DeneyimDuzenle?DID=@item.DID" class="btn btn-warning">Düzenle</a>
                                </div>
                            </td>



                        </tr>

                    }
                </tbody>
                <tfoot>
                    <tr>
                        <th>ID</th>
                        <th>Firma Adı</th>
                        <th>Unvan</th>
                        <th>Yıl Aralığı</th>
                        <th>Tecrübe</th>
                        <th>İşlem</th>
                    </tr>
                </tfoot>
            </table>
        </div>
        <!-- /.card-body -->
    </div>


</div>
